{
  "env": {
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-undef": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "ignorePatterns": ["*.config.js"]
}
